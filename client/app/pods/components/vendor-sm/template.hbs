
<!-- Table -->
<div class="panel panel-default">
  <!-- Default panel contents -->
  <div class="panel-overlay">
    {{#if vendor.phone}}
      <img src="assets/icons/heart.png">
    {{/if}}

  </div>
  <div class="panel-heading" id="banner">
    <div class="row">
      <div class="col-md-2">
        <div class="avatar-frame">
        <img id="logo" src="assets/images/75eweblogo.jpg">
        </div>
      </div>
      <div class="col-md-3">
        <h3 class="top50">{{vendor.name}}</h3>

      </div>
      <div class="col-md-2">
        <h5 class="top100">{{vendor.city}}, {{vendor.state}}</h5>
      </div>
    </div>
  </div>
  <div class="panel-body">
    <div class="row">
      <div class="col-md-2">
        <h3>{{vendor.rating}}</h3>

        <label><h5>RATING</h5></label>
      </div>
      <div class="col-md-2">
        <h3>3</h3>
        <label><h5>EMPOYEES</h5></label>
      </div>
      <div class="col-md-2">
        <h3>0</h3>
        <label><h5>OPPORTUNITIES</h5></label>
      </div>
      <div class="col-md-2">
        <H3>4</H3>
        <label><h5>TASKS</h5></label>
      </div>
      <div class="col-md-2">
      <h3>Ryan B.</h3>
        <label><h5>ACC MGR</h5></label>
      </div>
      <div class="col-md-2">

      </div>
    </div>
    <hr>
    <p>{{vendor.description}}</p>

    {{yield}}
  </div>

</div>
